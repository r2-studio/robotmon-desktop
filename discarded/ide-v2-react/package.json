{
  "name": "robotmon-developer",
  "productName": "Robotmon Developer",
  "version": "0.2.0",
  "description": "Robotmon Developer",
  "homepage": "./",
  "main": "main.js",
  "private": true,
  "author": "R2 Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/r2-studio/robotmon-desktop.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "electron": "^9.1.0",
    "electron-packager": "^9.1.0",
    "electron-rebuild": "^1.6.0",
    "electron-reload": "^1.2.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "adbkit": "^2.11.0",
    "brace": "^0.10.0",
    "electron-pref": "^1.1.3",
    "fbemitter": "^2.1.1",
    "func-pipe": "^1.0.4",
    "grpc": "^1.3.9",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-ace": "^5.5.0",
    "react-bootstrap": "^0.31.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.0.0"
  },
  "scripts": {
    "webpack-start": "./node_modules/.bin/webpack",
    "electron-start": "./node_modules/.bin/electron .",
    "start": "./node_modules/.bin/concurrently \"npm run webpack-start\" \"npm run electron-start\"",
    "build-win32": "electron-packager . --platform=win32 --arch=ia32 --overwrite --prune --icon=icons/icon.ico --ignore=app/src --out=out",
    "build-win64": "electron-packager . --platform=win32 --arch=x64 --overwrite --prune --icon=icons/icon.ico --ignore=app/src --out=out",
    "build-mac": "electron-packager . --platform=darwin --arch=x64 --overwrite --prune --icon=icons/icon.icns --ignore=app/src --out=out && electron-installer-dmg ./out/Robotmon\\ Developer-darwin-x64/Robotmon\\ Developer.app Robotmon\\ Developer --out=out --overwrite --icon=icons/icon.icns",
    "build-linux": "electron-packager . --platform=linux --arch=x64 --overwrite --prune --icon=icons/icon.png --ignore=app/src --out=out"
  }
}
