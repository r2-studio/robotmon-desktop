{
  "name": "robotmon-desktop",
  "version": "0.1.0",
  "description": "Robotmon desktop",
  "main": "main.js",
  "private": true,
  "author": "R2 Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/r2-studio/robotmon-desktop.git"
  },
  "devDependencies": {
    "electron-packager": "^9.0.1",
    "electron-rebuild": "^1.6.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron": "^9.1.0",
    "ejs": "^2.5.6",
    "glob": "^7.1.2",
    "grpc": "^1.3.9",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "punt": "^2.3.0"
  },
  "scripts": {
    "start": "electron .",
    "build-win32": "electron-packager . Robotmon --platform=win32 --arch=ia32 --overwrite --prune --icon=icons/icon.ico",
    "build-win64": "electron-packager . Robotmon --platform=win32 --arch=x64 --overwrite --prune --icon=icons/icon.ico",
    "build-mac": "electron-packager . Robotmon --platform=darwin --arch=x64 --overwrite --prune --icon=icons/icon.icns",
    "build-linux": "electron-packager . Robotmon --platform=linux --arch=x64 --overwrite --prune --icon=icons/icon.png"
  }
}
